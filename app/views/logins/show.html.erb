
<div class="user">
  <%= form_tag("/login/authenticate", method: "post") do %>
    <%= label_tag(:username, "Enter Username:") %>
    <%= text_field_tag(:username) %><br>
    <%= label_tag(:password, "Enter Password:") %>
    <%= password_field_tag(:password) %><br>
    <%= submit_tag("Login") %>
  <% end %>
</div>

<a href="<%= new_login_path %>">New User</a>
